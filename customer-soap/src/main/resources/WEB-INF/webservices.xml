<?xml version='1.0' encoding='UTF-8'?>
<webservices xmlns:javaee="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1">
    <webservice-description>
        <webservice-description-name>MyWebService</webservice-description-name>
        <port-component>
            <port-component-name>MyWebServiceSoapPort</port-component-name>
            <wsdl-port xmlns:an="http://com.tosan.customer_sample.soap/">an:MyWebServiceSoapPort</wsdl-port>
